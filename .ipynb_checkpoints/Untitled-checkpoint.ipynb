    "import pandas as pd \n",
    "\n",
    "df = pd.read_csv(\"data/time_confirmed.csv\")\n",
    "\n",
    "df = df.loc[df[\"Country/Region\"] == \"Afghanistan\"]\n",
    "\n",
    "df = df.drop(columns=[\"Province/State\", \"Country/Region\", \"Lat\", \"Long\"]).sum().reset_index(name=\"confirmed\")\n",
    "\n",
    "df"
   ]
  }
